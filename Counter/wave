#! /opt/homebrew/Cellar/icarus-verilog/11.0/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/11.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/11.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/11.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/11.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/11.0/lib/ivl/va_math.vpi";
S_0x123604700 .scope module, "counter_tb" "counter_tb" 2 2;
 .timescale -9 -12;
v0x6000000c4240_0 .var "clk", 0 0;
v0x6000000c42d0_0 .net "counter_out", 1 0, v0x6000000c4090_0;  1 drivers
v0x6000000c4360_0 .var "enable", 0 0;
v0x6000000c43f0_0 .var "reset", 0 0;
S_0x123604870 .scope module, "c1" "counter" 2 9, 3 1 0, S_0x123604700;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /OUTPUT 2 "count";
v0x6000000c4000_0 .net "clk", 0 0, v0x6000000c4240_0;  1 drivers
v0x6000000c4090_0 .var "count", 1 0;
v0x6000000c4120_0 .net "en", 0 0, v0x6000000c4360_0;  1 drivers
v0x6000000c41b0_0 .net "rst", 0 0, v0x6000000c43f0_0;  1 drivers
E_0x600003cc48a0 .event posedge, v0x6000000c4000_0;
    .scope S_0x123604870;
T_0 ;
    %wait E_0x600003cc48a0;
    %load/vec4 v0x6000000c41b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x6000000c4090_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6000000c4120_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x6000000c4090_0;
    %addi 1, 0, 2;
    %assign/vec4 v0x6000000c4090_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x123604700;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x123604700 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x123604700;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000000c4240_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000000c43f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000000c4360_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000000c4240_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000000c43f0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x123604700;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x6000000c4240_0;
    %inv;
    %store/vec4 v0x6000000c4240_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x123604700;
T_4 ;
    %delay 200000, 0;
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./Counter/tb/counter_tb.v";
    "./Counter/src/counter.v";
